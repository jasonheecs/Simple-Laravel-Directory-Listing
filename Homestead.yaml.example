---
ip: "192.168.10.10"
memory: 2048
cpus: 1
hostname: directory-listing
name: directory-listing
provider: virtualbox
mariadb: true

authorize: ~/.ssh/id_rsa.pub

keys:
    - ~/.ssh/id_rsa

folders:
    - map: "/Users/jasonhee/Projects/efusion-demo"
      to: "/home/vagrant/directory-listing"
      type: "nfs"

sites:
    - map: homestead.app
      to: "/home/vagrant/directory-listing/public"

databases:
    - homestead

